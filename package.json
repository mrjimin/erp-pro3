{
    "name": "my-admin",
    "version": "0.0.0",
    "description": "erp后台前端项目代码",
    "author": "zjm",
    "license": "MIT",
    "scripts": {
        "dev": "vite --force",
        "build": "vite build",
        "serve": "vite preview",
        "lint-fix": "eslint --fix --ext src/*.js --ext .ts --ext .jsx --ext .vue src/",
        "dep": "node ./command/route.js && vite --force",
        "route": "node ./command/route.js",
        "build-pro": "node ./command/deploy.js --production",
        "deploy": "vite build --mode test && vite build --mode production && node ./command/deploy.js --production && node ./command/deploy.js --test",
        "test": "vite build --mode test",
        "pro1": "vite build --mode pro1",
        "pro2": "vite build --mode pro2",
        "build-dev": "vite build --mode production && node ./command/upload.js --production",
        "build-test": "vite build --mode test && node ./command/upload.js --test",
        "build-pro1": "vite build --mode pro1 && node ./command/upload.js --pro1",
        "build-pro2": "vite build --mode pro2 && node ./command/upload.js --pro2",
        "publish:dev-test": "npm run build-dev && npm run build-test",
        "publish": "vite build --mode production && vite build --mode test && vite build --mode pro1 && vite build --mode pro2"
    },
    "dependencies": {
        "@element-plus/icons-vue": "^2.0.10",
        "@tinymce/tinymce-vue": "^5.0.0",
        "@vueuse/core": "^9.6.0",
        "axios": "^1.2.2",
        "default-passive-events": "^2.0.0",
        "echarts": "^5.3.2",
        "element-china-area-data": "^5.0.2",
        "element-plus": "^2.2.26",
        "js-base64": "^3.7.3",
        "js-cookie": "^3.0.1",
        "md5": "^2.3.0",
        "nprogress": "^0.2.0",
        "pinia": "^2.0.28",
        "sortablejs": "^1.15.0",
        "splitpanes": "^3.1.5",
        "swiper": "^8.2.4",
        "vue": "^3.2.45",
        "vue-clipboard3": "^2.0.0",
        "vue-router": "^4.1.5",
        "vue3-print-nb": "^0.1.4"
    },
    "devDependencies": {
        "@types/crypto-js": "^4.1.1",
        "@types/js-cookie": "^3.0.2",
        "@types/md5": "^2.3.2",
        "@types/node": "^18.11.15",
        "@types/nprogress": "^0.2.0",
        "@types/sortablejs": "^1.13.0",
        "@types/splitpanes": "^2.2.1",
        "@typescript-eslint/eslint-plugin": "^5.46.1",
        "@typescript-eslint/parser": "^5.46.1",
        "@vitejs/plugin-vue": "^3.1.2",
        "@vue/compiler-sfc": "^3.2.45",
        "eslint": "^8.29.0",
        "eslint-plugin-vue": "^9.8.0",
        "ftp": "^0.3.10",
        "prettier": "^2.8.1",
        "sass": "^1.56.2",
        "ssh2-sftp-client": "^9.0.4",
        "terser": "^5.16.1",
        "typescript": "^4.9.4",
        "unocss": "^0.47.6",
        "vite": "^3.2.5",
        "vite-plugin-compression": "^0.5.1",
        "vue-eslint-parser": "^9.1.0",
        "vue-tsc": "^1.0.13"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ],
    "engines": {
        "node": ">=12.0.0",
        "npm": ">= 6.0.0"
    }
}
